{
  "contacts": [
    {% if whatsAppIds is defined %}
    {% for contact in whatsAppIds %}
      {
        "profile": {
          "name": "{{profileName}}"
        },
        "wa_id": {{ contact | json }}
      }
    {% endfor %}
    {% endif %}
  ],
  "messages":[{
    {% if from is defined %}
    "from": "{{from}}",
    {% endif %}
    {% if messageId is defined %}
      "id": "{{messageId}}",
    {% endif %}
    "text": {
      "body": [
        "Namespace {{execution.namespace}}",
        "Flow ID {{execution.flowId}}",
        "Execution ID {{execution.id}}",
        "Execution Status {{execution.state.current}}",
        "*<{{link}}|[{{execution.namespace}}] {{execution.flowId}} âž› {{execution.state.current}}>*\n> {% if firstFailed == false %}Succeded{% else %}Failed on task `{{firstFailed.taskId}}`{% endif %} after {{duration}}"
        ]
    },
    "type": "text"
  }]
}